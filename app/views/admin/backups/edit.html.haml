.block
  .secondary-navigation
    %ul.wat-cf
      %li.first!= link_to "All backups", admin_backups_path
      %li!= link_to "Services", service_admin_backups_path
  .content
    %h2.title
      Editing backup 
      != @backup.account_number
    .inner
      - form_for [:admin, @backup], :html => { :class => :form } do |f|
        != render :partial => "form", :locals => {:f => f}

- content_for :sidebar, render(:partial => 'sidebar')
