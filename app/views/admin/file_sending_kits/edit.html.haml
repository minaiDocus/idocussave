.block
  .secondary-navigation
    %ul.wat-cf
      %li.first!= link_to "All file sending kits", admin_file_sending_kits_path
      %li!= link_to "Add file sending kit", new_admin_file_sending_kit_path
  .content
    %h2.title
      Editing file sending kit : 
      != @file_sending_kit.title
    .inner
      - form_for [:admin, @file_sending_kit], :html => { :class => :form } do |f|
        != render :partial => "form", :locals => {:f => f}

- content_for :sidebar, render(:partial => 'sidebar')
