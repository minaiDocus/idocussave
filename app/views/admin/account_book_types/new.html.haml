.block
  .secondary-navigation
    %ul.wat-cf
      %li.first!= link_to "All account book types", admin_account_book_types_path
      %li.active!= link_to "Add account book type", new_admin_account_book_type_path
  .content
    %h2.title New account book type
    .inner
      - form_for [:admin, @account_book_type], :html => { :class => :form } do |f|
        != render :partial => "form", :locals => {:f => f}

- content_for :sidebar, render(:partial => 'sidebar')
