.block
  .secondary-navigation
    %ul.wat-cf
      %li.first!= link_to "All subscriptions", admin_subscriptions_path
      %li!= link_to "All scan subscriptions", admin_scan_subscriptions_path
      %li.active!= link_to "Add scan subscription", new_admin_scan_subscription_path
  .content
    %h2.title
      Add subscription
    .inner
      - form_for [:admin, @scan_subscription], :html => { :class => :form } do |f|
        != render :partial => "form", :locals => {:f => f}
        
- content_for :sidebar, render(:partial => 'sidebar')
